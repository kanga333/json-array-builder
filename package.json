{
  "name": "json-array-builder",
  "version": "0.1.0",
  "private": true,
  "description": "Build JSON array from string",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanga333/json-array-builder.git"
  },
  "keywords": [
    "actions",
    "json"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^15.3.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vercel/ncc": "^0.28.5",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
